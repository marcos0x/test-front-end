{
  "name": "testfrontend",
  "version": "0.1.0",
  "description": "MercadoLibre Test Front-End",
  "private": true,
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "node scripts/build.js",
    "build-css": "node-sass-chokidar src/client/ -o src/client/",
    "watch-css": "npm run build-css && node-sass-chokidar src/client/ -o src/client/ --watch --recursive",
    "client": "node scripts/start.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "node scripts/test.js",
    "flow": "flow"
  },
  "author": "Marcos Avila",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/client/**/test/**/*.{js,jsx,mjs}",
      "<rootDir>/src/client/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "1.1.3",
    "cors": "^2.8.4",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.16.3",
    "flow-bin": "^0.81.0",
    "fs-extra": "3.0.1",
    "lodash": "^4.17.11",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^5.10.0",
    "redux-persist-transform-filter": "^0.0.18",
    "redux-thunk": "^2.3.0",
    "resolve": "1.6.0",
    "whatwg-fetch": "2.0.3",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "7.1.6",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "css-loader": "0.28.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.17.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "1.14.2",
    "react-dev-utils": "^5.0.1",
    "redux-devtools-extension": "^2.13.5",
    "regenerator-runtime": "^0.12.1",
    "remote-redux-devtools": "^0.5.13",
    "remotedev-server": "^0.2.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.2.0",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  }
}
